<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>

    <rule>
        <from>/resources/libs/highcharts/latest(.*)$</from>
        <to type="redirect">%{context-path}/resources/libs/highcharts/v8.0.0$1</to>
    </rule>
    
    <rule>
        <from>/resources/libs/highmaps/latest(.*)$</from>
        <to type="redirect">%{context-path}/resources/libs/highmaps/v8.0.0$1</to>
    </rule>
    
    <rule>
        <from>/resources/libs/highcharts-editor/latest(.*)$</from>
        <to type="redirect">%{context-path}/resources/libs/highcharts-editor/v0.3.0$1</to>
    </rule>

</urlrewrite>