<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    id="complementos-apps"
    version="2.5">

  	<servlet>
		<servlet-name>apis-styles</servlet-name>
		<jsp-file>/apis/styles.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>apis-styles</servlet-name>
		<url-pattern>/apis/styles.css</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>apps-footer</servlet-name>
		<jsp-file>/apps/footer.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>apps-footer</servlet-name>
		<url-pattern>/apps/footer.html</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>apps-navbar</servlet-name>
		<jsp-file>/apps/navbar.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>apps-navbar</servlet-name>
		<url-pattern>/apps/navbar.html</url-pattern>
	</servlet-mapping>
</web-app>
